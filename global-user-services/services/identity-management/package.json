{
  "name": "user-identity-managemant",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "set NODE_ENV=dev && nodemon src/index.ts",
    "start:debug": "node --inspect -r ts-node/register -r source-map-support/register src/index.ts",
    "test": "jest --silent --verbose --no-cache --detectOpenHandles --forceExit",
    "test:coverage": "jest --silent --coverage --no-cache",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start:prod": "pm2 start dist/index.js --name ident-manage",
    "stop:prod": "pm2 stop ident-manage",
    "build:docker": "docker build -t shubhamsbd/identity-manager .",
    "start:docker": "docker run -d -p 8000:8000 --name node-app --env-file .env shubhamsbd/identity-manager",
    "stop:docker": "docker stop node-app && docker rm node-app",
     "pactum": "jest --runInBand"
  },
  "keywords": [],
  "author": "HCL",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "axios-mock-adapter": "^2.1.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "mongodb-client-encryption": "^6.3.0",
    "mongodb-memory-server": "^11.0.1",
    "mongoose": "^8.20.0",
    "pactum": "^3.9.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
